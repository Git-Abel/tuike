                    $('#sortSeachSubmit').on('click',  function(event) {
                        event.preventDefault();
                        var jgqj1 = $('input[name="jgqj1"]').val();
                        var jgqj2 = $('input[name="jgqj2"]').val();
                        /* Act on the event */
                        if(jgqj1 == "" || jgqj2 == ""){
                            return false;
                        }
                        DTKsatc.eventStat({name:"CmsPcSortEvent",params:{desc:$('input[name="jgqj1"]').val()+'_'+$('input[name="jgqj2"]').val()}})
                    });
